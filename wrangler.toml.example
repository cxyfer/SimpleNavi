name = "simplenavi"
compatibility_date = "2024-01-01"
pages_build_output_dir = "dist"

# D1 Database
[[d1_databases]]
binding = "DB"
database_name = "your-database-name"
database_id = "your-database-id"

# KV Namespace (Favicon Cache)
[[kv_namespaces]]
binding = "FAVICON_CACHE"
id = "your-kv-namespace-id"
